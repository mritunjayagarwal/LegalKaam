        </div>
		<div class="row masonary-layout filter-layout">
			<% features.forEach((feature) => { %>
                <div class="col-md-4 col-sm-6 col-xs-12" style="border: 2px solid #3563a9;">
                    <div class="single-project-item">
                        <div class="single-service-page-box">
                            <div class="sec-title text-center">
                                <h2><%= feature.head %></h2>
                                <span class="decor-line">
                                    <span class="decor-line-inner"></span>
                                </span>
                            </div>
                            <p><%= feature.desc %></p>
                        </div><!-- /.single-service-page-box -->
                    </div><!-- /.single-project-item -->
                </div><!-- /.col-md-4 -->
            <% }) %>
		</div><!-- /.row -->
    </div><!-- /.container -->
</section><!-- /.sec-pad -->



